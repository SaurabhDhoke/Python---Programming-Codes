# Data of tuple immutable

Data = (10,20,30,40,"Marvellous",10)
print("Data type is : ",type(Data))
print("Elements are : ",Data)
print("0th element is : ",Data[0])
print("3rd element is : ",Data[3])
print("Last element is : ",Data[-1])

