# List
# sequential
# indexed
# data is mutable
# list is mutable
# allows duplicate
# Heterogenious



Data = [11,21,51,101,111]

print("Data[0] : ",Data[0])
print("Data[1] : ",Data[1])
print("Data[-1] : ",Data[-1])
print("Data[-2] : ",Data[-2])

print("Data[1:3]",Data[1:3])
print("Data[0:4]",Data[0:4])
