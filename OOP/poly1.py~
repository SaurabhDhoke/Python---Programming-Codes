"""
polymorphisom (method overloading)

Date : 09.01.2022
"""

class Demo:
	def __init__(self):
		self.i = 10
		self.j = 20
	
	def Add(self,a):
		print("Inside add with one parameter")
	
	def Add(self,a,b):
		print("Inside add with two paramters")


def main():
	obj = Demo()

if __name__ == "__main__":
	main()


