# Date : 08.01.2022

class Sparrow:
	def fly(self):
		print("Sparrow flying")

class Airplane:
	def fly(self):
		print("Airplane flying")

class Whale:
	def swim(self):
		print("Whale swimming")

def fun(entity):
	entity.fly()
		
sparrow = Sparrow()
airplane = Airplane()
whale = Whale()

fun(sparrow)
fun(airplane)
fun(whale)




